{
  "name": "podcst-web",
  "version": "1.0.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/{*.ts,*.tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
    "lint": "next lint",
    "deploy": "vercel",
    "prepare": "husky install"
  },
  "dependencies": {
    "@shopify/react-web-worker": "2.1.7",
    "axios": "0.21.4",
    "downshift": "6.1.7",
    "howler": "2.2.3",
    "idb-keyval": "6.0.2",
    "match-sorter": "6.3.1",
    "next": "12.0.5",
    "normalize.css": "8.0.1",
    "react": "18.0.0-alpha-4298ddbc5-20211023",
    "react-dom": "18.0.0-alpha-4298ddbc5-20211023",
    "react-virtual": "2.8.1",
    "swr": "1.0.1",
    "zustand": "3.6.5"
  },
  "devDependencies": {
    "@types/chromecast-caf-sender": "1.0.4",
    "@types/howler": "2.2.3",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/wicg-mediasession": "1.1.2",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.1",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  }
}
