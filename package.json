{
  "name": "podcst-web",
  "version": "2.3.1",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/{*.ts,*.tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
    "lint": "next lint",
    "short-url": "bun scripts/generate-short-url.ts"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.5.9",
    "downshift": "9.0.8",
    "howler": "2.2.4",
    "idb-keyval": "6.2.1",
    "ioredis": "5.4.2",
    "match-sorter": "8.0.0",
    "negotiator": "1.0.0",
    "next": "15.2.6",
    "normalize.css": "8.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-virtual": "2.10.4",
    "swr": "2.2.5",
    "xml2js": "0.6.2",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/chromecast-caf-sender": "1.0.11",
    "@types/eslint": "9.6.1",
