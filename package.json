{
  "name": "podcst-web",
  "version": "2.0.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/{*.ts,*.tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
    "lint": "next lint",
    "deploy": "vercel",
    "prepare": "husky install"
  },
  "dependencies": {
    "@shopify/react-web-worker": "5.0.13",
    "@vercel/analytics": "1.0.1",
    "@vercel/kv": "0.2.2",
    "downshift": "7.6.0",
    "howler": "2.2.3",
    "idb-keyval": "6.2.1",
    "match-sorter": "6.3.1",
    "next": "13.4.7",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-virtual": "2.10.4",
    "swr": "2.2.0",
    "xml2js": "0.6.0",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@types/chromecast-caf-sender": "1.0.5",
    "@types/howler": "2.2.7",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/wicg-mediasession": "1.1.4",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "5.1.5"
  }
}
