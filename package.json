{
  "name": "podcst-web",
  "version": "0.18.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/{*.ts,*.tsx}": [
      "prettier --write",
      "tslint --project tsconfig.json --fix",
      "git add"
    ]
  },
  "scripts": {
    "build:dev": "webpack --env.dev",
    "build:staging": "webpack --env.staging",
    "build:prod": "webpack --env.prod",
    "postbuild:staging": "./shell/fix-sw.sh staging",
    "postbuild:prod": "./shell/fix-sw.sh prod",
    "predeploy:staging": "yarn build:staging",
    "predeploy:prod": "yarn build:prod",
    "deploy:staging": "./shell/sync.sh staging",
    "deploy:prod": "./shell/sync.sh prod",
    "dev": "webpack-dev-server --config webpack.config.js --env.dev --hot --inline --host 0.0.0.0",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
    "lint": "tslint --project tsconfig.json --fix"
  },
  "dependencies": {
    "csstips": "0.3.0",
    "history": "4.7.2",
    "howler": "2.0.15",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.1.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "6.3.3",
    "typestyle": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@types/history": "4.7.2",
    "@types/howler": "2.0.5",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "@types/react-router-dom": "4.3.1",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.6.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.3",
    "lint-staged": "8.0.4",
    "prettier": "1.14.3",
    "ts-loader": "5.3.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.1.6",
    "url-loader": "1.1.2",
    "webpack": "4.24.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-plugin-hash-output": "3.1.0",
    "workbox-webpack-plugin": "3.6.3"
  }
}
