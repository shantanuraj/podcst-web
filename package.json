{
  "name": "podcst-web",
  "version": "0.6.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/{*.ts,*.tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
    "lint": "eslint src --ext .ts,tsx",
    "deploy": "vercel",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "0.21.1",
    "downshift": "6.1.7",
    "howler": "2.2.3",
    "idb-keyval": "5.1.3",
    "next": "11.1.0",
    "normalize.css": "8.0.1",
    "react": "18.0.0-alpha-e4e8226c6-20210812",
    "react-dom": "18.0.0-alpha-e4e8226c6-20210812",
    "react-virtual": "2.8.1",
    "swr": "0.5.6",
    "zustand": "3.5.8"
  },
  "devDependencies": {
    "@types/howler": "2.2.3",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/wicg-mediasession": "1.1.2",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  }
}
