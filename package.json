{
  "name": "podcst-web",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/{*.ts,*.tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
    "lint": "eslint src --ext .ts,tsx",
    "deploy": "vercel"
  },
  "dependencies": {
    "axios": "0.21.0",
    "next": "10.0.0",
    "normalize.css": "8.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "0.3.7"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "typescript": "4.0.5"
  }
}
